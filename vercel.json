{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.under-eight.vercel.app"
        }
      ],
      "destination": "https://under-eight.vercel.app/:path*",
      "permanent": true
    }
  ]
}
